    <script>
        (function ($) {
            $(document).ready(function() {
                $('#signup').on('click', function (e) {
                e.preventDefault();
                var checkbox = $('input[name=terms]').is(':checked');
                if(checkbox){
                    $('#signup').toggleDisableButton();
                    var fullName = $('input[name=fullName]').val();
                    var email = $('input[name=email]').val();
                    var organisationType = $('select[name=organisationType] option').filter(':selected').val();
                    var organisationName = $('input[name=organisationName]').val();
                    var organisationDescription = $('#organisationDescription').val();
                    var password = $('input[name=password]').val();

                    let values = {
                        fullName: fullName,
                        email: email,
                        organisationType: organisationType,
                        organisationName: organisationName,
                        organisationDescription: organisationDescription,
                        password: password
                    };

                    $.ajax({
                        url: 'https://www.angelinvestmentnetwork.com.pk/affiliate/ajax/register',
                        data: JSON.stringify(values),
                        method: 'POST',
                        contentType: 'application/json',
                        dataType: 'json',
                        success: function (response){
                            location.href = response;
                        },
                        complete: function (){
                            $('#signup').toggleDisableButton();
                        }
                    });
                }
                });

                $('#register input[name=email]').on('focusout', function() {
                    $.ajax({
                        url: 'https://www.angelinvestmentnetwork.com.pk/users/unique_email',
                        data: {
                            email: $('input[name=email]').val()
                        },
                        method: 'GET',
                        contentType: 'application/json',
                        dataType: 'json',
                        success: function (response){
                            if($('.error-email').is('html *')){
                                $('.error-email').remove();
                            }

                            $('#signup').prop('disabled', false);

                            if(response != true){
                                $('input[name=email]').parent().append(`
                                    <div class="error-email bg-red-100 text-red-500 p-2 my-1 rounded">${response}</div>
                                `);
                                $('#signup').prop('disabled', true);
                            }

                            if($('input[name=email]').val().length > 50){
                                $('input[name=email]').parent().append(`
                                    <div class="error-email bg-red-100 text-red-500 p-2 my-1 rounded">Email address is too long</div>
                                `);
                                $('#signup').prop('disabled', true);
                            }
                        }
                    });
                });

                $('input[name=confirmEmail]').on('cut copy paste', function (e) {
                    e.preventDefault();
                });

                $("#organization-types").on('change', function () {
                    var organisationType = $("#organization-types").val();
                    if (organisationType == '11') {
                        $(".organisation-details").hide();
                    } else {
                        $(".organisation-details").show();
                    }
                });

                $('#register input[name=confirmEmail]').on('focusout', function() {
                    var email = $('input[name=email]').val();
                    var confirmEmail = $(this).val();
                    if($('.error-confirmEmail').is('html *')){
                        $('.error-confirmEmail').remove();
                    }

                    $('#signup').prop('disabled', false);

                    if(email == confirmEmail){
                        $('#signup').prop('disabled', false);
                    } else {
                        $('input[name=confirmEmail]').parent().append(`
                            <div class="error-confirmEmail bg-red-100 text-red-500 p-2 my-1 rounded">Please make sure your email address match</div>
                        `);
                        $('#signup').prop('disabled', true);
                    }
                });
            })
            $("#organization-types").html($("#organization-types option").sort(function (a, b) {
                console.log("sada");
                return a.text == b.text ? 0 : a.text < b.text ? -1 : 1
            }));
        })(jq_symfony);
    </script>



