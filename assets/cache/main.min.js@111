if(type==='unread'){$(selector).removeClass('InboxConversation--read');$(selector).addClass('InboxConversation--unread');tt.data("type",'read');tt.data("tooltip",mark_read);tt.find(".material-icons").html('markunread');tt.attr('data-tooltip',mark_read).tooltip('show');}}});});$(document).on('click','.js-msg-archive',function(event){event.preventDefault();var me=$(this);var conid=me.data("id");var url=base_url+'inbox/set_archive/'+conid;var ind=$(".js-thread-"+conid).index()+1;$.ajax({url:url,type:'post',dataType:'json',success:function(response){if(response.status==='success'){$(".js-thread-"+conid).hide();if(($(".js-thread-"+conid).hasClass("InboxConversation--active"))||(me.hasClass(".js-header"))){$(".InboxConversation:eq("+ind+")").removeClass("InboxConversation--unread");$(".InboxConversation:eq("+ind+")").addClass("InboxConversation--active");loadMessage($(".InboxConversation:eq("+ind+")").find(".js-inbox-list").data("con-id"));}}}});});$(document).on('click','.js-nudge-btn',function(event){event.preventDefault();var _a=$(this);var url=_a.data("url");$.ajax({url:url,data:{ajax:"ajax"},type:'post',dataType:'json',success:function(response){if(response.status==='success'){loadMessage(_a.data("con-id"));}}});});var cons=0;$(document).on("click",'.js-connection-status',function(e){var proposal_id=$(this).data("proposal-id");var connection_id=$(this).data("connection-id");var role=$(this).data("role");var _t=$("#connection_status"+connection_id);console.log(_t.val());if(_t.val()==='6'){if(role=='investor'){$('#investment_project_id').val(proposal_id);$('#investment_connection_id').val(connection_id);$('#total-investment-modal').modal();}else{saveConnectionStatus(connection_id,'investor');}}else{if(cons===0){cons=1;var org_status=$('#status'+connection_id).val();getStatusMessage($(this),_t,function(msg){$("#con_"+connection_id).find(".ttTxt").html(msg);$('.tooltip_js_'+connection_id).html(msg);tooltipResetWidth(msg);});saveStatus(connection_id,role);}}});function getStatusMessage(currentObj,selectObj,callback){var role=currentObj.data('role');if(role=='investor'){role='inv';}else{role="ent";}
id=selectObj.val();$.ajax({type:"POST",cache:false,url:base_url+"connections/getConStatusMessage/"+id+"/"+role,dataType:"json",success:function(response){if(response.status=='success'){callback(response.msg);}}})}
function saveStatus(connection_id,role){var status_id=$('select#connection_status'+connection_id).val();var messages_flag=$('select#connection_status'+connection_id).hasClass("messages-list");if(status_id==7||status_id==1){feedback=$('#feedbackTA'+connection_id).val();if(!feedback){feedback="";}
if(role=='investor'){var MSG=fetchFeedbackMessages(connection_id,status_id);MSG.success(function(data){$('#investorNudgeModal').find(".modal-content").html(data);$('#investorNudgeModalBtn').trigger("click");})
return false;}}else{feedback="";}
if(role==='undefined')
role='';else{if(feedback=='')
feedback=0;}
$.ajax({type:"POST",cache:false,url:base_url+'connections/nudge_status/'+connection_id+"/"+status_id+"/"+feedback+"/"+role,start:Notifier.notify(labels.sending,'',Beard.root+'assets/img/loading1.gif'),dataType:"json",success:function(response){if(response.status=='success'){Notifier.success(response.msg);$('input.connection_status'+connection_id).addClass("display-none");$('#status'+connection_id).val(status_id);if(status_id==7){$('#js-project-'+connection_id).hide('slow');}
if(messages_flag||status_id==7){}}else{Notifier.error(response.msg);}
cons=0;}});}
$(document).on('click','.save_totalInvestments',function(){var params=$('#addTotalInvestmentsForm').serializeArray();var project_id=$('#investment_project_id').val();var connection_id=$('#investment_connection_id').val();url=base_url+'investor/mylist/totalInvestment/';$.ajax({url:url,method:'Post',dataType:'json',data:params,success:function(response){if(response.status==='success'){$('#total-investment-modal').modal('hide');saveStatus(connection_id,'investor');Notifier.success(response.message);$(document).find('#total_investments_js').html(response.invested);var invested=response.number_invested;$('#active-container-js').children('.initiate-blk').hide();$(document).find('#in_js').html(invested);var interested=response.number_interested;$(document).find('#int_counter_js').html(interested);$('#total-investment-modal').modal('hide');$('#active-container-js').prepend($('.js-proposal-'+project_id));}else{Notifier.error(response.message);}}});});function saveInterestedMessageStatus(connection_id,status_id){var connection_id=connection_id;$.ajax({type:"POST",cache:false,url:base_url+'connections/nudge_status/'+connection_id+'/'+status_id,start:Notifier.notify(labels.sending,'',Beard.root+'assets/img/loading1.gif'),dataType:"json",success:function(response){if(response.status=='success'){Notifier.success(response.msg);window.location.href=base_url+'inbox/messages/'+connection_id;}else{Notifier.error(response.msg);}}});}});
