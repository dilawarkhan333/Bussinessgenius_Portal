function decSlider(slider){var value=slider.slider("option","value");value=parseInt(value);if(value>=0){var step=getSliderStep(value);step=parseInt(step);slider.slider("option","value",value-step);}}
function incSlider(slider){var value=slider.slider("option","value");value=parseInt(value);if(value<=5000000){var step=getSliderStep(value);step=parseInt(step);slider.slider("option","value",value+step);}}
function decSliders(slider){var values=slider.slider("option","values")
if(values[0]>=0){var step=getSliderStep(values[0]);x=parseInt(values[0]-step);y=parseInt(values[1]);slider.slider("option","values",[x,y]);}}
function incSliders(slider){var values=slider.slider("option","values")
if(values[1]<=5000000){var step=getSliderStep(values[1]);x=parseInt(values[0]);y=parseInt(values[1]+step);slider.slider("option","values",[x,y]);}}
function setSliderStep(slider,value){var step=1000;if(value>0&&value<100000){slider.slider("option","step",1000);}else if(value>100000&&value<250000){slider.slider("option","step",10000);}else if(value>250000&&value<1000000){slider.slider("option","step",50000);}else if(value>1000000&&value<5000000){slider.slider("option","step",250000);}}
function setSliderValue(elem,value){var str=value+"";numdigits=str.length-2;zeros="";var reg=/\d{2}/;for(i=0;i<numdigits;i++){zeros+='0';}
curr=str.match(reg);if(!curr){curr=0;}else{curr=curr+zeros;}
$("#"+elem).val(curr);}
function getSliderStep(value){var step=1000;if(value>0&&value<100000){step=1000;}else if(value>100000&&value<250000){step=10000;}else if(value>250000&&value<1000000){step=50000;}else if(value>1000000&&value<5000000){step=250000;}
return step;}
