function tooltipResetWidth(text){if(text==null)text="";var wordsCount=text.split(' ').length;var linemax=400;var properWidth=Math.min(wordsCount*50,linemax)+"px";if(wordsCount==1)properWidth="auto";$(".ttTxt").css({width:properWidth});}
