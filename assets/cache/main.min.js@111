var defaultOption={O_separator:separator,O_separator_:separator_};$(document).ready(function(){$.fn.number_range=function(){return this.each(function(){option=defaultOption;var min_obj=$(this).find(".min_value");var max_obj=$(this).find(".max_value");var start=0;$(min_obj).add(max_obj).keydown(function(event){SkipChar(event);});$(min_obj).add(max_obj).on("keyup",function(event){var start=this.selectionStart,end=this.selectionEnd;$(min_obj).add(max_obj).parent().find('.error:not(input)').remove();validateNumbers();$(this).val(strSep($(this).val()));if(event.keyCode==8)
this.setSelectionRange(start,end);});function validateNumbers(){var minstr=$(min_obj).val().replaceAll(',',"").replace(/\./g,"");var maxstr=$(max_obj).val().replaceAll(',',"").replace(/\./g,"");;var min=parseInt(minstr);var max=parseInt(maxstr);if(min>max&&max>0){if($(min_obj).parent().find('.error:not(input)').length>0)
