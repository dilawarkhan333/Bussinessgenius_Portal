this.setSelectionRange(start,end);});$(this).on("blur",function(event){validateMax();$(this).val(strSep($(this).val()));});function validateMax(){var maxstr=$(max_obj).val().replaceAll(',',"").replace(/\./g,"");;var max=parseInt(maxstr);if(max>maxvalue){$(max_obj).val(maxvalue);}}});}});function strSep(somenum){if(somenum=="")
return;somenum=somenum.replaceAll(',',"").replace(/\./g,"");somenum=parseInt(somenum);somenum=somenum.formatMoney(0,defaultOption.O_separator_,defaultOption.O_separator);if(parseInt(somenum)!=0)
return somenum;else
return"0";}
Number.prototype.formatMoney=function(c,d,t){var n=this,c=isNaN(c=Math.abs(c))?2:c,d=d==undefined?".":d,t=t==undefined?",":t,s=n<0?"-":"",i=parseInt(n=Math.abs(+n||0).toFixed(c))+"",j=(j=i.length)>3?j%3:0;return s+(j?i.substr(0,j)+t:"")+i.substr(j).replace(/(\d{3})(?=\d)/g,"$1"+t)+(c?d+Math.abs(n-i).toFixed(c).slice(2):"");};String.prototype.replaceAll=function(find,replace){var str=this;return str.replace(new RegExp(find,'g'),replace);};$.validator.addMethod("isValidMinNumber",function(value,element){return validateNumbersRange(element);},'Please ensure the Minimum value you enter is no more than the Maximum value');$.validator.addMethod("NotZero",function(value,element){var num=parseInt(value);if(num==0)
return false;return true;},'This field is required.');function validateNumbersRange(num_obj){var str=$(num_obj).val().replaceAll(',',"").replace(/\./g,"");var num=parseInt(str);if($(num_obj).hasClass("min_value")){min=num;var max_obj=$(num_obj).parent().parent('.number_range').find('.max_value');var maxstr=$(max_obj).val().replaceAll(',',"").replace(/\./g,"");var max=parseInt(maxstr);$(num_obj).add(max_obj).parent().find('.error:not(input)').remove();if((min>max&&max>0)){return false;}
return true;}}
function SkipChar(event){if(event.keyCode==9||event.keyCode==37||event.keyCode==39||event.keyCode==188)
return;if(event.keyCode==16||event.keyCode==46||event.keyCode==8||event.keyCode==option.O_separator.charCodeAt(0)){}else{if(event.keyCode<95){if(event.keyCode<48||event.keyCode>57){event.preventDefault();}}else{if(event.keyCode<96||event.keyCode>105){event.preventDefault();}}}}
